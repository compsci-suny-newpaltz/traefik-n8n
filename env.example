# Example .env file for traefik-n8n setup
# Where persistent data should be stored
DATA_FOLDER=/srv/dockerstuff/traefik-n8n/data

# Your domain name (used for environment purposes only; apache handles SSL)
DOMAIN_NAME=example.com
WEBHOOK_URL=https://example.com/
N8N_EDITOR_BASE_URL=https://example.com/

# Used by n8n for webhook URLs, etc.
SUBFOLDER=
N8N_PATH=/
# Encryption key for n8n instance (change to a secure value)
N8N_ENCRYPTION_KEY=your-encryption-key

# PostgreSQL credentials
POSTGRES_USER=your_postgres_user
POSTGRES_PASSWORD=your_postgres_password
POSTGRES_DB=your_postgres_db

# N8N User Manager API - Secure API key for password management endpoints
N8N_USER_MANAGER_API_KEY=your-secure-api-key-change-this
